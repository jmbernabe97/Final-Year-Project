    private void HomeButtonActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        
        // remove panel
        MainPanel.removeAll();
        MainPanel.repaint();
        MainPanel.revalidate();
        
        // add panel
        MainPanel.add(HomePanel);
        MainPanel.repaint();
        MainPanel.revalidate();
    }                                          

