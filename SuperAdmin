    private void VO_SubmitButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // TODO add your handling code here:
        try{
            String url = "jdbc:derby://localhost:1527/VoteDatabase";
            String username = "vote";
            String password = "vote";
        
            Connection con = DriverManager.getConnection(url,username,password);
            Statement stat = con.createStatement();
            String Query = "INSERT INTO VOTE.VOTER (VOTERID, FIRSTNAME, SURNAME, CONTACTNUMBER, DATEOFBIRTH, ADDRESS, GENDER, EMAILADDRESS) VALUES "
                    + "('"+VO_VoterIDField.getText()+"','"+VO_FirstNameField.getText()+"','"+VO_SurnameField.getText()+"','"+VO_ContactNumberField.getText()+"'"
                    + ",'"+VO_DateOfBirthField.getText()+"','"+VO_AddressField.getText()+"','"+VO_GenderComboBox.getSelectedItem()+"',"
                    + "'"+VO_EmailAddressField.getText()+"')";
            
            stat.execute(Query);
            
            JOptionPane.showMessageDialog(null, "Voter has been registered");
            
            VO_VoterIDField.setText("");
            VO_FirstNameField.setText("");
            VO_SurnameField.setText("");
            VO_ContactNumberField.setText("");
            VO_DateOfBirthField.setText("");
            VO_AddressField.setText("");
            VO_EmailAddressField.setText("");
            VO_UsernameField.setText("");
            VO_PasswordField.setText("");
        }
        catch(SQLException ex){
            JOptionPane.showMessageDialog(null, ex.toString());
        }
        
    }                                               

    private void VO_ClearButtonActionPerformed(java.awt.event.ActionEvent evt) {                                               
        // TODO add your handling code here:
        VO_VoterIDField.setText("");
        VO_FirstNameField.setText("");
        VO_SurnameField.setText("");
        VO_ContactNumberField.setText("");
        VO_AddressField.setText("");
        VO_EmailAddressField.setText("");
    }                                              

