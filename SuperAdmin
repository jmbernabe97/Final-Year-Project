    private void SignOutButtonActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
        
        int DialogButton = JOptionPane.YES_NO_OPTION;
        int DialogResult = JOptionPane.showConfirmDialog(this, "Would you like to Sign Out?", "Sign Out", DialogButton);
        
        if(DialogResult==0){
            // true condition
            System.exit(0);
        }else{
            // false condition           
        }
    }                                             

