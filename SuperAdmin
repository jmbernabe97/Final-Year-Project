    private void VO_VoterIDFieldKeyReleased(java.awt.event.KeyEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void VO_FirstNameFieldKeyReleased(java.awt.event.KeyEvent evt) {                                              
        // TODO add your handling code here:
        String PATTERN="^[a-zA-Z]{0,27}$";
        Pattern patt=Pattern.compile(PATTERN);
        Matcher match=patt.matcher(VO_FirstNameField.getText());
        if(!match.matches()){
            VO_FirstNameError.setText("Name is incorrect!");
        }
        else{
            VO_FirstNameError.setText(null);
        }
    }                                             

    private void VO_SurnameFieldKeyReleased(java.awt.event.KeyEvent evt) {                                            
        // TODO add your handling code here:
        String PATTERN="^[a-zA-Z]{0,27}$";
        Pattern patt=Pattern.compile(PATTERN);
        Matcher match=patt.matcher(VO_SurnameField.getText());
        if(!match.matches()){
            VO_SurnameError.setText("Name is incorrect!");
        }
        else{
            VO_SurnameError.setText(null);
        }
    }                                           

    private void VO_ContactNumberFieldKeyReleased(java.awt.event.KeyEvent evt) {                                                  
        // TODO add your handling code here:
        String PATTERN="^[0-9]{0,11}$";
        Pattern patt=Pattern.compile(PATTERN);
        Matcher match=patt.matcher(VO_ContactNumberField.getText());
        if(!match.matches()){
            VO_ContactNumberError.setText("Contact Number is incorrect!");
        }
        else{
            VO_ContactNumberError.setText(null);
        }
    }                                                 

